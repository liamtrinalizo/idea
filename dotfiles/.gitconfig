[core]
	editor = vim
	autocrlf = true
    pager = "~/diff-so-fancy/diff-so-fancy | less --tabs=4 -RFX"
[user]
	email = Martinez-Le_Ninan.Jean@addi-data.com
	name = MJ093
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[alias]
    st = status
    ci = commit
    cim = commit -m
    co = checkout
    br = branch
    log1 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset)%C(dim white) %an%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold yellow)%d%C(reset)' --all
    log2 = log         --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim red)%an%C(reset)%C(bold yellow)%d%C(reset)' --all
	d = difftool
[winUpdater]
	recentlySeenVersion = 2.23.0.windows.1
[color]
	ui = true
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
